{
    "name": "tcgunel/shpr-bot",
    "description": "Ürünlerin kategorize olarak çeker.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tolga Can GÜNEL",
            "email": "info@mobius.studio"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "*",
        "guzzlehttp/guzzle": "*",
        "campo/random-user-agent": "^1.3"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TCGunel\\ShprBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TCGunel\\ShprBot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TCGunel\\ShprBot\\ShprBotServiceProvider"
            ],
            "aliases": {
                "ShprBot": "ShprBot"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/paratest --processes 6",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "brianium/paratest": "*"
    }
}
